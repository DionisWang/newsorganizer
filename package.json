{
    "name": "organizer",
    "version": "0.1.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "start": "node start.js",
        "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm audit fix --force && npm run build && npm run sitemap",
        "build-client": "cd client && npm run build && npm run sitemap",
        "client": "cd client && npm start",
        "server": "nodemon start.js",
        "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
        "sitemap": "cd client && npm run sitemap"
    },
    "dependencies": {
        "axios": ">=0.21.2",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.4",
        "cookie-session": "^1.4.0",
        "dotenv": "^8.2.0",
        "esm": "^3.2.25",
        "express": "^4.17.1",
        "mongoose": ">=5.9.0",
        "nodemon": "^2.0.2",
        "ssl-express-www": "^3.0.7",
        "uuid": "^3.4.0",
        "follow-redirects": ">=1.14.8",
        "lodash": ">=4.17.21",
        "ansi-regex": ">=5.0.1"      
    },
    "engines": {
        "node": "12.14.1"
    },
    "devDependencies": {
        "concurrently": "^5.2.0",
    }
}
